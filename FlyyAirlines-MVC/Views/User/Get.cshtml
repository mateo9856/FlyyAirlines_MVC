@model FlyyAirlines.Data.User

<div class="row text-center">
    <div class="col">
        <h2>E-mail</h2>
        <p>@Model.Email</p>
    </div>
    <div class="col">
        <h2>Nazwa użytkownika</h2>
        <p>@Model.UserName</p>
    </div>
</div>

<div class="row text-center">
    <div class="col">
        <h2>Imię</h2>
        <p>@Model.Name</p>
    </div>
    <div class="col">
        <h2>Nazwisko</h2>
        <p>@Model.Surname</p>
    </div>
</div>

<div class="row text-center">
    <div class="col">
        <h2>Rola</h2>
        <p>@Model.Role</p>
    </div>
    <div class="col">
        <h2>Data urodzenia</h2>
        <p>@Model.DateOfBirth</p>
    </div>
</div>